AA;AACA,UAAA,MAAA,GAAA;AACA,QAAA,SAAA,EAAA,QAAA,CAAA,cAAA,CAAA,QAAA,CADA;AAEA,QAAA,QAAA,EAAA,KAFA;AAGA,QAAA,IAAA,EAAA,IAHA;AAIA,QAAA,QAAA,EAAA,IAJA;AAKA,QAAA,aAAA,EAAA;AALA,OAAA;AAOA,MAAA,OAAA,CAAA,GAAA,iCAAA,MAAA,GAAA,KAAA,CAAA,MAAA;AACA,MAAA,MAAA,mCAAA,MAAA,GAAA,KAAA,CAAA,MAAA,CAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,MAAA;AACA,UAAA,IAAA;AACA,MAAA,IAAA,GAAA,MAAA,CAAA,aAAA,CAAA,MAAA,CAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,IAAA,EArBA,CAsBA;;AACA,MAAA,UAAA,CAAA,YAAA;AACA,QAAA,KAAA,CAAA,OAAA;AACA,OAFA,EAEA,KAAA,CAAA,IAAA,IAAA,KAFA,CAAA;AAGA,KA1BA;AA2BA;AAhDA,CAAA","sourcesContent":["<template>\n  <div class=\"_ba_animation\">\n    <div class=\"el-message-box\">\n      <div class=\"el-message-box__header\">\n        <div class=\"el-message-box__title\">\n          <span>{{ title }}</span>\n        </div>\n      </div>\n      <div class=\"el-message-box__content\">\n        <div class=\"el-message-box__container\">\n          <!---->\n          <div class=\"el-message-box__message\">\n            <p>\n              <strong>{{ text }}</strong>\n            </p>\n          </div>\n        </div>\n      </div>\n\n      <div class=\"_animation\" id=\"lottie\"></div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  props: {\n    type: String,\n    time: Number,\n    title: String,\n    text: String,\n    params:{}\n  },\n  methods: {\n    closeEl() {\n      this.$destroy();\n      try {\n        document.body.removeChild(this.$el);\n      } catch (error) {}\n    },\n  },\n  created() {\n    this.title ? \"\" : (this.title = \"操作提示\");\n    this.text ? \"\" : (this.text = \"表操作进行中\");\n  },\n  mounted() {\n    this.$nextTick(() => {\n      console.log(this.$global.animationJSON);\n      console.log(this.type);\n      console.log(this.$global.animationJSON[this.type]);\n      let animationData;\n      animationData = this.$global.animationJSON[\n        this.type ? this.type : \"save\"\n      ];\n      console.log(animationData);\n      var params = {\n        container: document.getElementById(\"lottie\"),\n        renderer: \"svg\",\n        loop: true,\n        autoplay: true,\n        animationData: animationData,\n      };\n      console.log({...params,...this.params})\n      params={...params,...this.params}\n      console.log(params)\n      var anim;\n      anim = lottie.loadAnimation(params);\n      console.log(anim);\n      // 1s后消失\n      setTimeout(() => {\n        this.closeEl();\n      }, this.time || 10000);\n    });\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n._ba_animation {\n  width: 100vw;\n  height: 100vh;\n  position: fixed;\n  left: 0;\n  top: 0;\n  z-index: 2018;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  background: rgba(76, 76, 76, 0.3);\n  .el-message-box {\n    padding-bottom: 60px;\n    .el-message-box__message {\n      min-height: unset;\n    }\n  }\n}\n._animation {\n  width: 10vw;\n  margin: auto;\n}\n</style>\n"],"sourceRoot":"src/components/iconSvg"}]}